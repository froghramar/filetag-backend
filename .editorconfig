# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Matches multiple files with brace expansion notation
# Set default charset
[*.{js,json,css}]
charset = utf-8

# 4 size tab indentation
[*.{js,json,css}]
indent_style = tab
tab_width = 4
